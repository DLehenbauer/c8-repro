{
  "name": "c8-repro",
  "version": "0.1.0",
  "description": "Repro for Issue #302",
  "homepage": "https://github.com/DLehenbauer/c8-repro",
  "repository": "https://github.com/DLehenbauer/c8-repro",
  "license": "MIT",
  "author": "Microsoft and contributors",
  "main": "index.js",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist/lib",
    "dist/types",
    "index.js"
  ],
  "scripts": {
    "build": "npm run webpack",
    "clean": "rimraf nyc dist *.tsbuildinfo *.build.log",
    "test": "jest",
    "test:coverage": "c8 npm run test -- --ci --reporters=default --reporters=jest-junit",
    "webpack": "webpack --config webpack.prod.js --env.production"
  },
  "c8": {
    "all": true,
    "cache-dir": "nyc/.cache",
    "exclude": [
      "**/*.d.ts",
      "**/src/test/**/*.ts",
      "**/dist/test/**/*.js",
      "experimental/examples/**",
      "**/*.bundle.js"
    ],
    "exclude-after-remap": false,
    "extension": [
      ".ts",
      ".tsx",
      ".js",
      ".jsx"
    ],
    "include": [
      "src/**/*.ts",
      "dist/**/*.js"
    ],
    "report-dir": "nyc/report",
    "reporter": [
      "cobertura",
      "html",
      "text"
    ],
    "temp-directory": "nyc/.nyc_output"
  },
  "dependencies": {
    "@hig/fonts": "^1.0.2",
    "@material-ui/core": "^4.0.0",
    "@material-ui/lab": "^4.0.0-alpha.16",
    "@material-ui/styles": "^4.1.1",
    "classnames": "^2.2.6",
    "memoize-one": "^5.0.0",
    "react-base-table": "1.0.2",
    "react-loading-skeleton": "^1.1.2",
    "react-select": "^2.4.3",
    "react-virtualized-auto-sizer": "^1.0.2",
    "ts-loader": "^8.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/preset-env": "^7.2.0",
    "@hig/fonts": "^1.0.2",
    "@material-ui/core": "^4.0.0",
    "@material-ui/lab": "^4.0.0-alpha.16",
    "@material-ui/styles": "^4.1.1",
    "@storybook/addon-actions": "^6.1.21",
    "@storybook/addon-essentials": "^6.1.21",
    "@storybook/addon-links": "^6.1.21",
    "@storybook/react": "^6.1.21",
    "@types/jest": "22.2.3",
    "@types/react": "^16.9.15",
    "@types/underscore": "^1.9.1",
    "async": "^3.2.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "babel-plugin-istanbul": "^5.1.0",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-plugin-polyfill-corejs2": "^0.1.10",
    "babel-plugin-polyfill-corejs3": "^0.1.7",
    "babel-plugin-polyfill-regenerator": "^0.1.6",
    "babel-plugin-transform-jsx": "^2.0.0",
    "base64-js": "1.3.0",
    "body-parser": "^1.18.3",
    "c8": "^7.7.1",
    "clean-webpack-plugin": "^3.0.0",
    "concurrently": "^5.2.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "eslint": "~7.18.0",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.3",
